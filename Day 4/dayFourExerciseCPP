#include <stdio.h>
#include <iostream>

using namespace std;

int main () {

    cout << "Enter number 1: ";
    int num1;
    cin >> num1;

    cout << "Enter number 2: ";
    int num2;
    cin >> num2;

    if ( num1 == num2 ) {
        cout << "Both the numbers are same!" << endl;
    } else if ( num1 > num2) {
        cout << num1 << " is greater of the two!" << endl;
    } else {
        cout << num2 << " is greater of the two!" << endl;
    }


    return 0;
}
